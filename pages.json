{
  "easycom": {
    "^u-(.*)": "@/uview-ui/components/u-$1/u-$1.vue"
  },
  "pages": [
    //pages数组中第一项表示应用启动页，参考：https://uniapp.dcloud.io/collocation/pages
    {
      "path": "pages/home/homePage",
      "style": {
        "navigationBarTitleText": "首页",
        "enablePullDownRefresh": false,
        "navigationStyle": "custom"
      }
    },
    {
      "path": "pages/setMeal/setMeal",
      "style": {
        "navigationBarTitleText": "套餐预定",
        "enablePullDownRefresh": false
      }
    },
    {
      "path": "pages/reserve/reserve",
      "style": {
        "navigationBarTitleText": "我的预定",
        "enablePullDownRefresh": false
      }
    },
    {
      "path": "pages/personal/personal",
      "style": {
        "navigationBarTitleText": "个人中心",
        "enablePullDownRefresh": false
      }
    }
  ],
  "globalStyle": {
    "navigationBarTextStyle": "black",
    "navigationBarTitleText": "",
    "navigationBarBackgroundColor": "#fff",
    "backgroundColor": "#F7F7F7"
  },
  "tabBar": {
    "color": "#A6ABB5",
    "selectedColor": "#0BB9C8",
    "borderStyle": "white",
    "backgroundColor": "#ffffff",
    "list": [
      {
        "pagePath": "pages/home/homePage",
        "iconPath": "static/images/tabbar/home.png",
        "selectedIconPath": "static/images/tabbar/home_selected.png",
        "text": "首页"
      },
      {
        "pagePath": "pages/setMeal/setMeal",
        "iconPath": "static/images/tabbar/demo.png",
        "selectedIconPath": "static/images/tabbar/demo_selected.png",
        "text": "套餐预定"
      },
      {
        "pagePath": "pages/reserve/reserve",
        "iconPath": "static/images/tabbar/manage.png",
        "selectedIconPath": "static/images/tabbar/manage_selected.png",
        "text": "我的预定"
      },
      {
        "pagePath": "pages/personal/personal",
        "iconPath": "static/images/tabbar/user.png",
        "selectedIconPath": "static/images/tabbar/user_selected.png",
        "text": "个人中心"
      }
    ]
  }
}
